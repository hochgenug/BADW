{
  "manifest_version": 2,
  "name": "Betaseries Automatic Download Workflow",
  "description": "This extension help you to get episodes from betaseries.com faster.",
  "version": "1.13",
  "icons": {
    "16": "images/icon16.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "run_at": "document_idle",
      "matches": [
        "https://www.betaseries.com/membre/*/series"
      ],
      "js": [
        "scripts/lib/jquery-3.2.1.min.js",
        "scripts/functions.js",
        "scripts/betaseries.js"
      ]
    },
    {
      "run_at": "document_idle",
      "matches": [
        "*://*.annuaire-telechargement.com/index.php*"
      ],
      "js": [
        "scripts/lib/jquery-3.2.1.min.js",
        "scripts/functions.js",
        "scripts/at-search.js"
      ]
    },
    {
      "run_at": "document_idle",
      "matches": [
        "*://*.annuaire-telechargement.com/telecharger-series/*"
      ],
      "js": [
        "scripts/lib/jquery-3.2.1.min.js",
        "scripts/functions.js",
        "scripts/at-series.js"
      ]
    },
    {
      "run_at": "document_idle",
      "matches": [
        "*://extreme-download.club/**"
      ],
      "js": [
        "scripts/lib/jquery-3.2.1.min.js",
        "scripts/functions.js",
        "scripts/ed-protect.js"
      ]
    },
    {
      "run_at": "document_idle",
      "matches": [
        "*://ed-protect.org/**"
      ],
      "js": [
        "scripts/lib/jquery-3.2.1.min.js",
        "scripts/functions.js",
        "scripts/ed-protect.js"
      ]
    },
    {
      "run_at": "document_idle",
      "matches": [
        "*://*/*"
      ],
      "js": [
        "scripts/lib/jquery-3.2.1.min.js",
        "scripts/dl-protect.js"
      ]
    },
    {
      "run_at": "document_idle",
      "matches": [
        "https://login.uptobox.com/",
        "*://uptobox.com/*"
      ],
      "js": [
        "scripts/lib/jquery-3.2.1.min.js",
        "scripts/uptobox.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    "scripts/injectBetaseries.js"
  ]
}