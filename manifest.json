{
  "manifest_version": 2,
  "name": "Betaseries Automatic Download Workflow",
  "description": "This extension help you to get episodes from betaseries.com faster.",
  "version": "1.1",
  "content_scripts": [{
    "run_at": "document_end",
    "matches": ["https://www.betaseries.com/membre/*/series"],
    "js": ["scripts/lib/jquery-3.2.1.min.js", "scripts/functions.js", "scripts/betaseries.js"]
  }, {
    "run_at": "document_end",
    "matches": ["*://*.zone-telechargement.ws/index.php*"],
    "js": ["scripts/lib/jquery-3.2.1.min.js", "scripts/functions.js", "scripts/zt-search.js"]
  }, {
    "run_at": "document_end",
    "matches": ["*://*.zone-telechargement.ws/telecharger-series/*"],
    "js": ["scripts/lib/jquery-3.2.1.min.js", "scripts/functions.js", "scripts/zt-series.js"]
  }, {
    "run_at": "document_end",
    "matches": ["*://*.protecte-link.com/*"],
    "js": ["scripts/lib/jquery-3.2.1.min.js", "scripts/protect-zt.js"]
  }, {
    "run_at": "document_end",
    "matches": ["https://login.uptobox.com/", "*://uptobox.com/*"],
    "js": ["scripts/lib/jquery-3.2.1.min.js", "scripts/uptobox.js"]
  }],
  "web_accessible_resources": ["scripts/injectBetaseries.js"]
}